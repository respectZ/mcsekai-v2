{
  "format_version": "1.12.0",
  "animation_controllers": {
    "controller.animation.mcsekai.song.0038": {
      "states": {
        "default": {
          "transitions": [
            {
              "playing": "q.property('mcsekai:is_playing')"
            }
          ]
        },
        "playing": {
          "transitions": [
            {
              "playing.1": "q.property('mcsekai:song_character') == 1"
            },
            {
              "playing.2": "q.property('mcsekai:song_character') == 2"
            },
            {
              "playing.3": "q.property('mcsekai:song_character') == 3"
            }
          ]
        },
        "playing.1": {
          "animations": [
            "song.fix",
            "song.0038.01"
          ],
          "transitions": [
            {
              "default": "!q.property('mcsekai:is_playing')"
            }
          ]
        },
        "playing.2": {
          "animations": [
            "song.fix",
            "song.0038.02"
          ],
          "transitions": [
            {
              "default": "!q.property('mcsekai:is_playing')"
            }
          ]
        },
        "playing.3": {
          "animations": [
            "song.fix",
            "song.0038.03"
          ],
          "transitions": [
            {
              "default": "!q.property('mcsekai:is_playing')"
            }
          ]
        }
      }
    }
  }
}